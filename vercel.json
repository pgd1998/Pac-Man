{
    "version": 2,
    "builds": [
      {
        "src": "back-end/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "front-end/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "front-end/build"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/back-end/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/front-end/build/$1"
      }
    ]
  }